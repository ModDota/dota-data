{
  "name": "dota-data",
  "version": "0.9.1",
  "description": "Auto-generated Dota 2 resource dumps",
  "homepage": "https://github.com/ark120202/dota-data#readme",
  "bugs": "https://github.com/ark120202/dota-data/issues",
  "repository": "https://github.com/ark120202/dota-data/tree/master/packages/dota-data",
  "license": "Apache-2.0",
  "author": "ark120202",
  "files": [
    "lib/**/*.{js,ts}",
    "files/**/*.{json,ts}"
  ],
  "scripts": {
    "build": "run-s build:clean build:static build:tsc",
    "build:clean": "rimraf \"{files,lib}/**/*\"",
    "build:static": "ts-node build",
    "build:tsc": "tsc",
    "dev": "run-p dev:static dev:tsc",
    "dev:static": "nodemon",
    "dev:tsc": "tsc --watch --preserveWatchOutput"
  },
  "nodemonConfig": {
    "exec": "ts-node --transpile-only build",
    "ext": "ts,json",
    "watch": [
      "build",
      "src/schema",
      "src/schemas",
      "../dumper/dump"
    ]
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "fs-extra": "^8.1.0",
    "got": "^10.0.1",
    "lodash": "^4.17.15",
    "tslib": "^1.10.0",
    "vdf-extra": "^2.2.2"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^8.0.1",
    "dedent": "^0.7.0"
  }
}
